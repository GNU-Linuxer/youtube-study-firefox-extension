{
  "description": "UW study.",
  "author": "Mozilla_prerna",
  "manifest_version": 2,
  "name": "Template demo",
  "version": "0.3.1",
  "homepage_url": "https://github.com/prerna79/ion-basic-study",

   "permissions": ["geolocation", "webRequest", "*://api.bigdatacloud.net/*", "tabs"],

  "options_ui": {
    "page": "public/index.html",
    "open_in_tab": true
  },

  "content_scripts":[
    {
        "matches": ["*://*.youtube.com/*"],
        "js": ["dist/content-script.js"]
    }
],

  "background": {
    "scripts": ["dist/background.js"]
  }
}
